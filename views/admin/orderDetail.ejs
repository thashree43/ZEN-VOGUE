    <style>
        .header {
            padding: 20px;
            background-color: #000000;
            color: #fff;
        }

        .footer {
            padding: 20px;
            background-color: #000000;
            color: #fff;
            text-align: center;
        }

        h2 {
            border-bottom: 2px solid #ccc;
            padding-bottom: 20px;
        }

        .table-container {
            border: 1px solid #ccc;
            border-radius: 10px;
            overflow: hidden;
        }

        .table-container {
            box-shadow: 0px 4px 8px rgba(0, 0, 0, 0.1);
        }

        .table th {
            background-color: #f2f2f2;
            font-weight: bold;
            text-transform: uppercase;
            font-size: 22px;
        }

        .table tr {
            transition: background-color 0.2s ease;
        }

        .table tr:hover {
            background-color: #f9f9f9;
        }

        .genric-btn {
            display: inline-block;
            padding: 8px 16px;
            border: 2px solid #000;
            border-radius: 5px;
            background-color: #fff;
            color: #000;
            font-size: 12px;
            text-decoration: none;
            transition: background-color 0.2s ease;
        }

        .genric-btn:hover {
            background-color: #f2f2f2;
        }


        .status-label {
            display: inline-block;
            padding: 6px 10px;
            border-radius: 20px;
            font-size: 12px;
            font-weight: bold;
        }

        .status-placed {
            color: #1e3a2b;
            background-color: #89dd9f;
            border-radius: 20px;

        }

        .status-canceled {
            color: #ff2200;
            background-color: #fccfcf;
            padding: 10px 12px;
            border-radius: 20px;

        }

        .status-pending {
            color: #f57c00;
            background-color: #ffdbb5;
            border-radius: 20px;

        }

        .status-approved {
            color: #4a90e2;
            background-color: #b8dbf3;
            border-radius: 20px;

        }

        .status-delivered {
            color: #d3d3d3;
            background-color: #c5cae9;
            border-radius: 20px;

        }

        .btn-danger {
            background-color: #ff0000;
            color: #fff;
            border-radius: 20px;
            padding: 10px 12px;
            font-size: 12px;
        }

        .btn-primary {
            background-color: #04b104;
            color: #fff;
            border-radius: 20px;
            padding: 6px 12px;
            font-size: 12px;
        }

        .btn-secondary {
            background-color: #ff9800;
            color: #fff;
            border-radius: 20px;
            padding: 6px 12px;
            font-size: 12px;
        }

        tbody tr:hover {
            background-color: #f5f5f5;
        }

        td a.order-link {
            color: #616161;
            font-weight: 600;
            text-decoration: none;
            transition: color 0.3s ease;
        }

        td a.order-link:hover {
            color: #0056b3;
        }

        td.price {
            color: #272727;
        }

        td.status-cell {
            padding: 12px 0;
        }

        td.status-cell:hover .status-label {
            transform: scale(1.05);
        }

        td.action-cell {
            padding: 12px 0;
        }

        td.action-cell a.canceled-label {
            color: #d61b00;
            background-color: #fccfcf;
            padding: 4px 10px;
            border-radius: 20px;
            font-size: 10px;
        }

        td.action-cell a span.btn-secondary,
        td.action-cell a span.btn-primary {
            font-size: 13px;
            padding: 10px 12px;
            border-radius: 20px;
        }

        td.action-cell a span.btn-secondary+span,
        td.action-cell a span.btn-primary+span {
            margin-left: 10px;
        }

        .pagination {
            margin-top: 20px;
        }

        .pagination .page-item {
            display: inline-block;
            margin: 0 4px;
        }

        .pagination .page-link {
            border-radius: 20px;
            padding: 5px 10px;
            color: #333;
            background-color: #fff;
            border: 1px solid #ccc;
        }

        .pagination .page-link:hover {
            background-color: #f0f0f0;
        }

        .pagination .page-item.active .page-link {
            background-color: #007bff;
            color: #fff;
            border-color: #007bff;
        }

        .pagination .page-item.disabled .page-link {
            color: #ccc;
            background-color: transparent;
            border-color: transparent;
            cursor: not-allowed;
        }
    </style>
</head>

