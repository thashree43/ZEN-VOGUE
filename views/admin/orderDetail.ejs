            <main class="h-full pb-16 overflow-y-auto">
                <div class="container grid px-6 mx-auto">
                    <h2 class="my-6 text-3xl font-semibold text-gray-700 dark:text-gray-200">
                        Order Details
                    </h2>
            
                    <div class="w-full overflow-hidden rounded-lg shadow-xs">
                        <div class="w-full overflow-x-auto">
                            <table class="w-full whitespace-no-wrap">
                                <thead>
                                    <tr class="text-sm font-semibold tracking-wide text-left text-gray-500 uppercase border-b dark:border-gray-700 bg-gray-50 dark:text-gray-400 dark:bg-gray-800">
                                        <th class="px-4 py-3">SL</th>
                                        <th class="px-4 py-3">Order Id</th>
                                        <th class="px-4 py-3">Price</th>
                                        <th class="px-5 py-3">paymentMethod</th>
                                        <th class="px-5 py-3">Status</th>
                                        <th class="px-5 py-3">Order Actions</th>
                                    </tr>
                                </thead>
            
                                <tbody class="bg-white divide-y dark:divide-gray-700 dark:bg-gray-800">
                                    <% if (orders.length > 0) { %>
                                        <% for (let i = 0; i < orders.length; i++) { %>
                                            <tr class="text-gray-700 dark:text-gray-400">
                                                <td class="px-4 py-3" style="color: #000;">
                                                    <%= i + 1 %>
                                                </td>
                                                <td class="px-4 py-3" style="color: #000;">
                                                    <a href="/admin/viewOrders?id=" class="order-link"></a>
                                                    <%= orders[i]._id %>
                                                </td>
                                                <td class="px-4 py-3 price">
                                                    ₹<%= orders[i].subtotal %>
                                                </td>
                                                <td class="px-5 py-4 price">
                                                    <%= orders[i].paymentmethod %>
                                                </td>
                                                <td class="px-5 py-3 status-cell">
                                                    <% if (
                                                        orders[i].status === "placed" ||
                                                        orders[i].status === "delivered" ||
                                                        orders[i].status === "waiting for approval" ||
                                                        orders[i].status === "Return Approved"
                                                    ) { %>
                                                        <a href="/admin/orderStatus?id=<%= orders[i]._id %>">
                                                            <span class="status-label status-placed">
                                                                <%= orders[i].status %>
                                                            </span>
                                                        </a>
                                                    <% } else if (orders[i].status === "cancelled") { %>
                                                        <a>
                                                            <span class="status-canceled px-3 py-1 text-xs font-semibold leading-tight text-gray-800 bg-gray-200 rounded-full dark:text-gray-100 dark:bg-gray-700">
                                                                <%= orders[i].status %>
                                                            </span>
                                                        </a>
                                                    <% } else if (orders[i].status === "pending") { %>
                                                        <a href="/admin/orderStatus?id=<%= orders[i]._id%>">
                                                            <span class="status-pending px-3 py-1 text-xs font-semibold leading-tight text-orange-800 bg-orange-200 rounded-full dark:text-white dark:bg-orange-600">
                                                                <%= orders[i].status %>
                                                            </span>
                                                        </a>
                                                    <% } %>
                                                </td>
                                                <td class="px-4 py-3 action-cell">
                                                    <% if (orders[i].status === "cancelled") { %>
                                                        <a class="px-2 py-1 font-semibold leading-tight text-orange-700 bg-orange-100 rounded-full dark:text-white dark:bg-orange-600">
                                                            Canceled
                                                        </a>
                                                    <% } else if (
                                                        orders[i].status !== "delivered" &&
                                                        orders[i].status !== "waiting for approval" &&
                                                        orders[i].status !== "Return Approved" &&
                                                        orders[i].status !== "pending"
                                                    ) { %>
                                                        <a href="/admin/orderCancel?id=<%= orders[i]._id%>">
                                                            <span class="btn-danger">Cancel</span>
                                                        </a>
                                                        <% if (
                                                            orders[i].status === "placed" ||
                                                            orders[i].status === "delivered" &&
                                                            orders[i].status !== "delivered" &&
                                                            orders[i].status !== "pending"
                                                        ) { %>
                                                            <a href="/admin/orderDeliverd?id=<%= orders[i]._id%>">
                                                                <span class="btn-primary">Delivered</span>
                                                            </a>
                                                        <% } %>
                                                    <% } else if (orders[i].status === "Return Approved") { %>
                                                        <a>
                                                            <span class="text-success font-semibold leading-tight text-gray-700 bg-gray-100 rounded-full dark:text-gray-100 dark:bg-gray-700">
                                                                Approved
                                                            </span>
                                                        </a>
                                                    <% } else if (
                                                        orders[i].status !== "delivered" &&
                                                        orders[i].status !== "pending"
                                                    ) { %>
                                                        <a href="/admin/orderDeliverd?id=<%= orders[i]._id%>">
                                                            <span class="btn-secondary">Approve</span>
                                                        </a>
                                                    <% } %>
                                                </td>
                                            </tr>
                                        <% } %>
                                    <% } else { %>
                                        <tr>
                                            <td colspan="6" class="text-center">
                                                <h1>No orders found</h1>
                                            </td>
                                        </tr>
                                    <% } %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </main>
            
